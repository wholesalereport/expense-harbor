    "@prisma/client": "6.1.0",
    "@sendgrid/mail": "^8.1.4",
    "@stripe/react-stripe-js": "^3.0.0",
    "@stripe/stripe-js": "^5.2.0",
    "@vercel/kv": "^3.0.0",
    "compromise": "^14.14.3",
    "framer-motion": "^11.11.17",
    "iron-session": "^8.0.4",
    "lodash": "^4.17.21",
    "next": "15.0.3",
    "openai": "^4.76.0",
    "papaparse": "^5.4.1",
    "raw-body": "^3.0.0",
    "react": "19.0.0-rc-66855b96-20241106",
